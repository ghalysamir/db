@{
}
@model User
@* @Model
@Model.GetType() *@
<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title">@Model.Name</h5>
        <p class="card-text">Age:@Model.Age</p>
    </div>
    <ul class="list-group list-group-flush">
        <li class="list-group-item">Email:@Model.Email</li>
        <li class="list-group-item">Password:@Model.Password</li>
      
    </ul>
    <div class="card-body">
        <a href="/User/Index" class="card-link">Go Back</a>
    </div>
</div>
